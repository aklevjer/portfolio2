{
  "name": "portfolio2",
  "private": true,
  "version": "0.0.0",
  "description": "A personal portfolio website showcasing my projects and experience",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier -w src/**/*.{ts,tsx}",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint-fix": "eslint src/**/*.{ts,tsx} --cache --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aklevjer/portfolio2.git"
  },
  "keywords": [
    "React",
    "Typescript",
    "TailwindCSS",
    "Vite"
  ],
  "author": "Aleksander Klevjer <alekle53052@stud.noroff.no>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aklevjer/portfolio2/issues"
  },
  "homepage": "https://github.com/aklevjer/portfolio2#readme",
  "dependencies": {
    "@tailwindcss/vite": "^4.0.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "tailwindcss": "^4.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwind-merge": "^3.0.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
